# Environment Variables Template
# Copy this file to .env and update with your actual credentials

# ETG/RateHawk API Credentials
ETG_API_KEY_ID=your_etg_key_id
ETG_API_KEY_SECRET=your_etg_secret
ETG_API_BASE_URL=https://api-sandbox.worldota.net/api/b2b/v3

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_key

# Flask Configuration
FLASK_ENV=development
FLASK_DEBUG=True
FLASK_SECRET_KEY=your_secret_key_here

# Server Configuration
HOST=0.0.0.0
PORT=5000

# Razorpay Payment Gateway
# Get your keys from: https://dashboard.razorpay.com/app/keys
RAZORPAY_KEY_ID=rzp_test_xxxxxxxxxxxxxxxx
RAZORPAY_KEY_SECRET=your_razorpay_secret_here
RAZORPAY_WEBHOOK_SECRET=your_webhook_secret_here
